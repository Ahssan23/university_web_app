<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Admissions</title>
  <script src="https://cdn.tailwindcss.com"></script>
</head>
<body class="bg-gray-100 min-h-screen font-sans">
  <div class="max-w-4xl mx-auto py-10 px-4">
    <h1 class="text-3xl font-bold mb-8 text-center text-green-900">Admissions List</h1>

    <div class="grid grid-cols-1 md:grid-cols-2 gap-6">
      <% admissions.forEach((element) => { %>
        <div class="bg-white shadow-lg rounded-2xl p-6 hover:shadow-xl transition duration-300">
          <h2 class="text-xl font-semibold text-gray-800 mb-2"><%= element.name %></h2>
          <p class="text-gray-600"><strong>CNIC:</strong> <%= element.cnic %></p>
          <p class="text-gray-600"><strong>Father Name:</strong> <%= element.fathername %></p>
          <p class="text-gray-600"><strong>Birth:</strong> <%= element.birth %></p>
          <p class="text-gray-600"><strong>Address:</strong> <%= element.address %></p>
          <p class="text-gray-600"><strong>City:</strong> <%= element.city %></p>
        </div>
      <% }) %>
    </div>
  </div>
</body>
</html>
